{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "mcasperson/pressgangccms:v1.1",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "ContainerDirectory": "/var/www/html",
      "HostDirectory": "/var/www/html",
    },
    {
      "ContainerDirectory": "/var/database",
      "HostDirectory": "/var/database",
    },
    {
      "ContainerDirectory": "/var/pressganglogs",
      "HostDirectory": "/var/pressganglogs",
    }
  ],
  "Logging": "/var/pressganglogs"
}



